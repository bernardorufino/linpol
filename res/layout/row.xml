<?xml version="1.0" encoding="UTF-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <EditText
        android:id="@+id/property"
        style="@style/Cell.Property"/>

    <EditText
        android:id="@+id/left_value"
        style="@style/Cell.Value.Side"/>

    <EditText
        android:id="@+id/interpol_value"
        style="@style/Cell.Value.Interpol"/>

    <EditText
        android:id="@+id/right_value"
        style="@style/Cell.Value.Side"/>

    <TextView
        android:id="@+id/delete"
        style="@style/Cell.DeleteButton"/>

    <TextView
        android:id="@+id/control"
        style="@style/Cell.ControlButton"
        android:visibility="gone"/>

</TableRow>